UXTerm*faceName: Inconsolata
! UXTerm*faceSize: 13
XTerm*faceName:  Inconsolata
! XTerm*faceSize:  11

URxvt*font: xft:Inconsolata-zi4:pixelsize=22
! URxvt*font: xft:DejaVu Sans Mono:pixelsize=22
! URxvt*faceSize: 15
URxvt*transparent: false
URxvt*shading: 0
XTerm*ScrollKey: true
XTern*sixelScrolling: true

XTerm*termName:  xterm-256color
UXTerm*termName: xterm-256color
URxvt.termName:  rxvt-unicode-256color
URxvt*scrollBar: false

URxvt.perl-ext-common: resize-font

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up

URxvt*scrollTtyOutput:  false
URxvt*scrollWithBuffer: false

! Xterm escapes, word by word movement
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
! URxvt.perl-ext-common: default,-readline

! URxvt.perl-ext-common: default,matcher
! URxvt.url-launcher: /usr/bin/xdg-open
! URxvt.matcher.button: 1
! URxvt.matcher.rend.0: Uline Bold fg5

! added from http://comments.gmane.org/gmane.comp.terminal-emulators.tmux.user/6813
! *URxvt*keysym.F1:		\033OP
! *URxvt*keysym.F2:		\033OQ
! *URxvt*keysym.F3:		\033OR
! *URxvt*keysym.F4:		\033OS



*foreground:	#ebebeb
*background:	#101012
*cursorColor:   red

