UXTerm*faceName:       Inconsolata
XTerm*faceName:        Inconsolata

!URxvt*font:            xft:Inconsolata-zi4:pixelsize=19
URxvt*font:            xft:Iosevka Extended:pixelsize=17
!URxvt*transparent:     false
!URxvt*shading:         0
XTerm*ScrollKey:       true
XTern*sixelScrolling:  true

URxvt.internalBorder:  8

XTerm*termName:        xterm-256color
UXTerm*termName:       xterm-256color
URxvt.termName:        rxvt-unicode-256color
URxvt*scrollBar:       false
URxvt*geometry:        100x25

URxvt.perl-ext-common: resize-font

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up

URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c200

! Xterm escapes, word by word movement
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

*foreground:	#ebebeb
*background:	#101012
*cursorColor:   red

! black
*color0:	#1F1F1F
*color8:	#4d4d4d
! redc
*color1:	#A54242
*color9:	#CC6666 
! green
*color2:	#B7CE42
*color10:	#BDE077
! yellow
*color3:	#FEA63C
*color11:	#FFE863
! blue
*color4:	#66AABB
*color12:	#AACCBB
! magenta
*color5:	#B7416E
*color13:	#BB4466
! cyan
*color6:	#5E7175
*color14:	#A3BABF
! white
*color7:	#DDEEDD
*color15:	#6C887A
