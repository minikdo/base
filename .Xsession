
# source my env

if [ -f ~/.zsh_my_env ]; then
    . ~/.zsh_my_env
fi

[[ -f ~/.Xmodmap ]] && xmodmap ~/.Xmodmap

xset s 1200 1200

xset dpms 1200 1200 1200

xss-lock slock &

emacs --daemon

urxvtd -q -o -f &

xrdb -merge ~/.Xdefaults 

/usr/bin/redshift -l $COORD &

synclient TapButton1=0 TapButton2=0 TapButton3=0 \
          PalmDetect=1

# exec awesome >~/.xsession.log 2>&1
exec xmonad
