
# source my env

if [ -f ~/.zsh_my_env ]; then
    . ~/.zsh_my_env
fi

[[ -f ~/.Xmodmap ]] && xmodmap ~/.Xmodmap

#light-locker --lock-on-suspend &
#xautolock -time 10 -locker slock &
xss-lock slock &

xrdb -merge ~/.Xdefaults 

$HOME/bin/redshift_launcher start &

synclient TapButton1=0 TapButton2=0 TapButton3=0

xrandr --output VIRTUAL1 --off --output eDP1 --primary --mode 1920x1080 --pos 0x1440 --rotate normal --output DP1 --mode 2560x1440 --pos 0x0 --rotate normal --output HDMI2 --off --output HDMI1 --off --output DP2 --off

exec awesome

